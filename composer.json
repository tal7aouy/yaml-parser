{
    "name": "tal7aouy/yaml-parser",
    "description": "A YAML parser package for open api",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "yaml",
        "parser",
        "parse",
        "yaml parse",
        "php"
    ],
    "authors": [
        {
            "name": "Mhammed Talhaouy",
            "email": "tal7aouy@gmail.com",
            "role": "PHP Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/tal7aouy/yaml-parser/issues",
        "source": "https://github.com/tal7aouy/yaml-parser"
    },
    "autoload": {
        "psr-4": {
            "Tal7aouy\\YamlParser\\": "src/"
        }
    },
    "require": {
        "php": "^8.1",
        "symfony/yaml": "^6.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.8",
        "pestphp/pest": "^1.21",
        "phpstan/phpstan": "^1.5",
        "symfony/var-dumper": "^6.0"
    },
    "scripts": {
        "test": [
            "@composer install",
            "vendor/bin/phpunit"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    }
}